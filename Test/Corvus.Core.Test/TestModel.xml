<?xml version="1.0" encoding="utf-16"?>
<Schema Namespace="Corvus.App" xmlns="http://docs.oasis-open.org/odata/ns/edm">
	<EntityType Name="Customer">
		<Key>
			<PropertyRef Name="Id" />
		</Key>
		<Property Name="Id" Type="Edm.Guid" Nullable="false" />
		<Property Name="Id2" Type="Edm.Guid" Nullable="false" />
		<Property Name="Name" Type="Edm.String" Nullable="false" />
		<Property Name="Mail" Type="Edm.String" Nullable="false" />
		<Property Name="ZipCode" Type="Edm.String" />
		<Property Name="Address" Type="Edm.String" />
		<NavigationProperty Name="Orders" Type="Collection(Corvus.App.Order)" Partner="Customer" />
	</EntityType>
	<EntityType Name="Product">
		<Key>
			<PropertyRef Name="Id" />
		</Key>
		<Property Name="Id" Type="Edm.Guid" Nullable="false" />
		<Property Name="Name" Type="Edm.String" Nullable="false" />
		<Property Name="Kind" Type="Edm.String" Nullable="false" />
		<Property Name="Price" Type="Edm.Int32" />
		<NavigationProperty Name="OrderDetails" Type="Collection(Corvus.App.OrderDetail)" Partner="Product" />
	</EntityType>
	<EntityType Name="Order">
		<Key>
			<PropertyRef Name="Id" />
		</Key>
		<Property Name="Id" Type="Edm.Guid" Nullable="false" />
		<Property Name="CustomerId" Type="Edm.Guid" Nullable="false" />
		<Property Name="OrderDate" Type="Edm.DateTimeOffset" Nullable="false" />
		<Property Name="TotalPrice" Type="Edm.Decimal" />
		<NavigationProperty Name="Customer" Type="Corvus.App.Customer" Nullable="false" Partner="Orders">
			<ReferentialConstraint Property="CustomerId" ReferencedProperty="Id2" />
		</NavigationProperty>
		<NavigationProperty Name="OrderDetails" Type="Collection(Corvus.App.OrderDetail)" Partner="Order" />
	</EntityType>
	<EntityType Name="OrderDetail">
		<Key>
			<PropertyRef Name="Id" />
		</Key>
		<Property Name="Id" Type="Edm.Guid" Nullable="false" />
		<Property Name="OrderId" Type="Edm.Guid" Nullable="false" />
		<Property Name="ProductId" Type="Edm.Guid" Nullable="false" />
		<Property Name="Price" Type="Edm.Decimal" />
		<NavigationProperty Name="Order" Type="Corvus.App.Order" Nullable="false" Partner="OrderDetails">
			<ReferentialConstraint Property="OrderId" ReferencedProperty="Id" />
		</NavigationProperty>
		<NavigationProperty Name="Product" Type="Corvus.App.Product" Nullable="false" Partner="OrderDetails">
			<ReferentialConstraint Property="ProductId" ReferencedProperty="Id" />
		</NavigationProperty>
	</EntityType>
	<EntityContainer Name="Default">
		<EntitySet Name="Customers" EntityType="Corvus.App.Customer">
			<NavigationPropertyBinding Path="Orders" Target="Orders" />
		</EntitySet>
		<EntitySet Name="Orders" EntityType="Corvus.App.Order">
			<NavigationPropertyBinding Path="Customer" Target="Customers" />
			<NavigationPropertyBinding Path="OrderDetails" Target="OrderDetails" />
		</EntitySet>
		<EntitySet Name="OrderDetails" EntityType="Corvus.App.OrderDetail">
			<NavigationPropertyBinding Path="Order" Target="Orders" />
			<NavigationPropertyBinding Path="Product" Target="Products" />
		</EntitySet>
		<EntitySet Name="Products" EntityType="Corvus.App.Product">
			<NavigationPropertyBinding Path="OrderDetails" Target="OrderDetails" />
		</EntitySet>
	</EntityContainer>
</Schema>